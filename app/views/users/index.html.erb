
<h1>ユーザー一覧</h1>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name%></td>
        <td><%= user.email %></td>
        <td><%= link_to 'Show', profile_path %></td>
        <td><%= link_to 'Edit', edit_profile_path(user) %></td>
        <td><%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

